{
    "expo": {
        "name": "front-end",
        "slug": "front-end",
        "version": "1.0.0",
        "orientation": "portrait",
        "scheme": "frontend",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/icon.svg",
                "backgroundColor": "#272727"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.platypus_3141592.frontend"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/icon.svg",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#272727"
                }
            ], [
            "expo-font",
            {
                "fonts": [
                    "./assets/fonts/Poppins-Bold.ttf",
                    "./assets/fonts/Poppins-SemiBold.ttf",
                    "./assets/fonts/Poppins-Medium.ttf",
                    "./assets/fonts/Poppins-Regular.ttf"
                ],
                "android": {
                    "fonts": [
                        {
                            "fontFamily": "Poppins",
                            "fontDefinitions": [
                                {
                                    "path": "./assets/fonts/Poppins-Bold.ttf",
                                    "weight": 700
                                },
                                {
                                    "path": "./assets/fonts/Poppins-SemiBold.ttf",
                                    "weight": 600
                                },
                                {
                                    "path": "./assets/fonts/Poppins-Medium.ttf",
                                    "weight": 500
                                },
                                {
                                    "path": "./assets/fonts/Poppins-Regular.ttf",
                                    "weight": 400
                                }
                            ]
                        }
                    ]
                }
            }
            ], [
                "expo-screen-orientation",
                {
                    "initialOrientation": "PORTRAIT"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
